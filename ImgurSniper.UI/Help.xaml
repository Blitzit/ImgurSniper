<Window x:Class="ImgurSniper.UI.Help"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:ImgurSniper.UI"
		xmlns:cultureStrings="clr-namespace:ImgurSniper.UI.Properties"
		mc:Ignorable="d"
		Title="{x:Static Member=cultureStrings:strings.help}" Height="400" Width="600">
	<Grid>
		<TabControl>
			<TabItem Header="General">
				<Label Content="Content goes here..." />
			</TabItem>
			<TabItem Header="Security" />
			<TabItem Header="Details" />
		</TabControl>
		
		
		<StackPanel HorizontalAlignment="Stretch">
			<Label></Label>
			
		</StackPanel>
	</Grid>
</Window>
